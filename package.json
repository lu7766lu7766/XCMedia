{
  "name": "tmp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build && yarn uProject",
    "build:rc": "vue-cli-service build && yarn uProject:rc",
    "build:stable": "vue-cli-service build && yarn uProject:stable",
    "//": "uProject",
    "uProject": "node build/s3sync",
    "uProject:rc": "node build/s3sync -rc",
    "uProject:stable": "node build/s3sync -stable",
    "///": "uResource",
    "uResource": "node build/s3sync -d resource -D resource",
    "uResource:rc": "node build/s3sync -rc -d resource -D resource",
    "uResource:stable": "node build/s3sync -stable -d resource -D resource",
    "uResource:all": "yarn uResource && yarn uResource:rc && yarn uResource:stable"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^3.2.0",
    "bootstrap-vue": "^2.1.0",
    "bytes": "^3.1.0",
    "core-js": "^3.4.3",
    "dual-listbox-vue": "^1.0.2",
    "jactools": "^3.2.0",
    "numeral": "^2.0.6",
    "tinymce": "^5.2.0",
    "vee-validate": "^3.2.5",
    "vue": "^2.6.10",
    "vue-bootstrap-datetimepicker": "^5.0.1",
    "vue-bus": "^1.2.1",
    "vue-filter-number-format": "^2.0.0",
    "vue-input-tag": "^2.0.6",
    "vue-jstree": "^2.1.6",
    "vue-router": "^3.1.3",
    "vue-search-select": "^2.9.3",
    "vue-sweetalert2": "^2.1.5",
    "vue2-editor": "^2.10.2",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "jac-s3-sync-aws": "^2.0.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  }
}
